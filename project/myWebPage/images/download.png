<script type="text/javascript">
	require(['jquery', 'goatee', 'plugins_nav_custom_booking_offers'], function ($, goatee, customOffers) {
		// allow pages to add offers to this template
		// these will come from listing/event detail widgets or book nav offer items (collection template)
		
		var offerItemTemplate = $('[data-sv-offerItemTemplate]').html();
		var $customOffersContainer = $('[data-sv-custom-offers]');
		var $customOfferList = $('[data-sv-custom-offerlist]');

		// append all existing offers (from widgets that have already loaded)
		var offers = customOffers.getOffers();
		offers.forEach(displayOffer);

		// watch for and append any new offers (bc the order widgets load can vary)
		customOffers.onAdd(displayOffer);

		// display a single custom offer added from the page
		function displayOffer (offerData) {
			if($customOffersContainer.hasClass('hide')){
				var $parent = $customOffersContainer.parent();
				// initialize when the first offer is added
				// detach and move to the top right position
				// (needs to start at the end due to CSS block grid weirdness)
				$customOffersContainer.detach().removeClass('hide');
				$parent.prepend($customOffersContainer);
			}
			// fill the template and append the offer to the container
			$customOfferList.append(goatee.fill(offerItemTemplate, offerData));
			$(document).trigger('translate');
		}
	});
</script>
