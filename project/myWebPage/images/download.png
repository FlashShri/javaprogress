      var rpStr = localStorage.recentpages;
      var rpArray = rpStr.length === 0 || rpStr === undefined ? [] : JSON.parse(rpStr);

      if(rpArray.length > 0) {
        rpArray.forEach(function(page) {
          if(page.image !== "") {
            if(page.image.indexOf("cloudinary") < 0) {
              page.cloudinaryImage = sv_cloudinaryLib.default.createResource({ imageUrl : page.image });
