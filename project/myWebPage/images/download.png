	/*
	*	@T201431: GET PAGE DATA FOR RECENT PAGES
	*	Used for building the data for the recent pages collections widget
	*/

	/* @T318932 is localStorage available */
	if(clientLib.localStorageAvailable()) {
		$("html").bind("saveRecentPage", function(){
			var cp = window.currentPage;

			// if we never got a title, then use the fist h1 found
			if(cp.title === "" || cp.title === undefined) {
				if($("h1").length > 0) {
					cp.title = $("h1").first().text();
