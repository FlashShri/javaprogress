				}
			}
			else { //open dropdown
				// 359066 preserve the previous open state of the menu
				wasOpen = $('body').hasClass('active-drop');

				// 359066 preserve the previous playing state of the slideshow
				var btnType = $clickedBtn.attr('data-header-btn');
				if (['book','search','menu'].indexOf(btnType) !== -1) {
					$('html').trigger('openMenu');

					// don't overwrite the playing state if the menu was already open because the video will always be paused
					if (!wasOpen) {
						wasPlaying = $('.custom_header_slideshow_homepage').hasClass('video-playing');
					}

					$("html").trigger("pauseVideo");

					$('.custom_header_slideshow_homepage').removeClass('video-playing');
				}

				// account for scrollbar width to prevent things from shifting when the menu opens
				if (!hasCrisisAlert) {
					headerButtons.style.transform = "translateX(-" + scrollbarWidth + "px)";
					bodyTag.style.paddingRight = scrollbarWidth + 'px';
