		bodyTag.classList.remove('norwaynow-drop');

		if(!$clickedBtn.hasClass("nearme")) {
			if($clickedBtn.is('.active')) { //close dropdown/reset
				$('html').trigger('closeMenu');
				// prevent things from shifting when the menu closes
				// note: this logic is replicated on the 'x' close button click handler below
				headerButtons.style.transform = "translateX(0)";
				bodyTag.style.paddingRight = 0;
				mainLogo.style.transform = "translateX(0)";

