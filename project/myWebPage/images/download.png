	core
) {
	var sb = $('#search_widget_headerbox_11a3ae3d-1dd1-43e8-a686-4c88e83f6f8a');
	var smb = $('#search_widget_mobilebox_11a3ae3d-1dd1-43e8-a686-4c88e83f6f8a');
	var form = sb.find('[data-sv-searchForm]');
	var mform = smb.find('[data-sv-searchForm]');
	var inp = form.find('[data-sv-searchInput]');
	var minp = mform.find('[data-sv-searchInput]');
	var items = sb.find('[data-sv-searchResultsItems]');
	var mitems = smb.find('[data-sv-searchResultsItems]');
	var curUrl = urlLib.parse(window.location.toString());
	var searchLabel = $("[data-search-label]").data("search-label") || form.find("input.search-label").val() || curUrl.get.searchLabel;
	var locale_code = $('html').prop('lang');
	var nav_section = "main";
	var section_mapping = {"system_business":{"business":"en","main":"primary"},"main_business_en":{"business":"en","main":"primary"},"main_business_no":{"business":"no","main":"no"},"main_business_ru":{"business":"ru","main":"ru"}};

	// @T208172 - Local search for premium partners
	if(searchLabel) {
		searchLabel = searchLabel.replace(/<[^>]+>/g, '');
		var placeholder = form.find(".searchInput").prop("placeholder");
		var searchValue = $.trim(curUrl.get.q).replace(/<[^>]+>/g, '');
		form.add(mform).find("input.search-label").val(searchLabel);
		form.add(mform).find(".searchInput").prop("placeholder", placeholder + " " + searchLabel);
		sb.find('.search-all').addClass('show').click(function() { $(this).prop('href', '/search/?q=' + form.find(".searchInput").val()); });
		smb.find('.search-all').addClass('show').click(function() { $(this).prop('href', '/search/?q=' + mform.find(".searchInput").val()); });
	}

	gtmDataLayer.push({event:'conditionalPageview'});

	var templates = {
		itemsTemplate : sb.find("[data-sv-itemsTemplate]").html(),
		mitemsTemplate : smb.find("[data-sv-itemsTemplate]").html()
	};

	var customSettings = {"navSearch":"/search/"};
	customSettings.types = {
		"blog_articles" : {
			"label" : "Articles",
			"name" : "blog_articles"
		},
		"blog_leisure_blog" : {
			"label" : "Leisure Blog",
			"name" : "blog_leisure_blog"
		},
		"events" : {
			"label" : "Events",
			"name" : "events"
		},
		"listings" : {
			"label" : "Listings",
			"name" : "listings"
		},
		"mediagallery" : {
			"label" : "Media Gallery",
			"name" : "mediagallery"
		},
		"nav" : {
			"label" : "Pages",
			"name" : "nav"
		},
		"offers" : {
			"label" : "Special Offers",
			"name" : "offers"
		}
	}

	var facets = {};
	$.each(customSettings.types, function(i,v){
		facets[v.name] = v.label;
	});

	var searchLayout = new search.SearchLayout({facets : facets});
	sb.data("sv_searchLayout", searchLayout);

	var imgParams = {
		width : 274,
		height : 137,
		quality : 65,
		crop : 'fill',
		sv : { gravity : 'focalPoint' }
	};

	// update the image to use the data src; add a class to make it fade in
	var loadImage = function() {
		var img = sb.find('.search_result_hero img');
		img.attr('src', img.attr('data-src')).addClass('loaded');
	};

	// retrieve a new image that uses focalPoint cropping
	var getImage = function(id) {
		// map the image_id to a mongo object id
		var filter = {
			_id : { '$oid' : id }
		};

		var options = {
			castDocs : false,
			limit : 1,
			fields : {
				_id : 1,
				resource : 1,
			}
		};

		$.get('/includes/rest_v2/plugins_assets_images/find/', {
			json : JSON.stringify({
				filter : filter,
				options : options
			}),
			token : core.simpleToken
		}).done(function(data) {
			if (data.docs && data.docs.length) {
				// build the new image with focal point cropping and
				// replace the existing image with the new one
				if (data.docs[0].resource) {
					var src = cloudinaryLib.default.createResource(data.docs[0].resource).getThumbnailUrl(imgParams);
					if (src) {
						sb.find('.search_result_hero img').attr('data-src', src).addClass('loaded');
					}
				}
			}

			loadImage();
		});
	};

	var runSearch = function(getObj) {
		var query = getObj;
		// @T236955 ignoring extra whitespace
		query.q = $.trim(query.q).replace(/\s+/g, ' ');
		var origQ = query.q;
		// filter query
		var fq = [];

		// @T446763 Hide UDF Field Direct links from list views
		fq.push("-custom_string_isdirectlink:Yes");

		// @T464119 - exclude package, tour operator, and travel provider listings
		fq.push("-custom_integer_typeid:2");
		fq.push("-custom_integer_typeid:3");
		fq.push("-custom_integer_typeid:4");


		//@T215360: limit results by site
		// filters for items which belong on this site

		if (sv_site.site === 'business') {
			// build the custom section parameter. default to English if in the system section.
			var section_business = (nav_section === 'system_business') ? 'en' : section_mapping[nav_section].business;
			var section_string = 'custom_string_nav_section:main_business_' + section_business + ' AND ';
			fq.push('+((' + section_string + 'sites:business AND type:nav) OR (sites:business AND sites:' + section_mapping[nav_section].main + ' AND type:(listings OR events)))');
		} else {
			fq.push("+sites:"+sv_site.site);
		}

		if (query.type) {
			// filters for items of a single type (pages, listings, events, blog, etc)
			fq.push("+type:"+query.type);
		}

		// query fields ( list of fields to test against search term )
		var qf;

		// phrase fields (doesn't change what documents match, but gives a boost if all of the terms occur next to or near each other, based on "ps" (phrase/proximity slop.))
		var pf;

		// qf as it appears in core, for reference (check core if updating)
		var core_qf = "titleExact^3 title^2 decodedURL^1.5 content altcontent_0 altcontent_1 keywords^0.5";

		// add custom paid keywords field
		qf = core_qf + ' custom_arr_text_paid_keywords^10'; // strong boost as these are highest priority
		pf = core_qf + ' custom_arr_text_paid_keywords^100'; // extra boost if there is a phrase match

		// add other custom fields to both query and phrase fields per client request
		qf += ' custom_text_municipality custom_text_destination';
		pf += ' custom_text_municipality custom_text_destination';


		// @T238910 mobile header search
		// @T286506 removing these for now, as we've finely tuned the main algorithm
		// if(mobile) {
		// 	qf = qf.replace(/title\^\d+/, 'title^10');
		// 	qf = qf.replace(/titleExact\^\d+/, 'titleExact^15');
		// }

		var solrArgs = {
			lang: $('html').attr('lang'),
			start : query.start,
			q : origQ.toLowerCase(),
			fq : fq.join(" "),
			qf : qf,
			pf : pf
		};

		searchLayout.search(solrArgs, function(err, rtn) {
			rtn.processed.request.q = origQ;

			delete rtn.processed.request.type;
			if (query.type) {
				rtn.processed.request.type = { label : facets[query.type], value : query.type };
			}

			postSearch(err, rtn);
		});
	};

	var postSearch = function(err, rtn) {
		if (err) { console.log(err); alert(err); }
		var resultCount = rtn.processed.response.docs.length;

		// @T384404 - use language specific detail path on the business site
		if (sv_site.site === 'business') {
			rtn.processed.response.docs = rtn.processed.response.docs.map(function (item) {
				item.encodedURL = item.encodedURL.replace('/listings/', '/' + locale_code + '/listings/');
				item.encodedURL = item.encodedURL.replace('/event/', '/' + locale_code + '/event/');
				return item;
			});
		}

		var pathPrefix = '';
		// @T384404 business microsite
		if (sv_site.site === 'business') {
			// when searching from system pages such as /listings/ it won't have a language so default to English
			pathPrefix = (nav_section === 'system_business') ? '/en' : '/' + section_mapping[nav_section].business;
		}

		// @T273588 create return obj to pass locale code in addition to request and response data to template
		var rtnObj = { request : rtn.processed.request, response : rtn.processed.response, locale_code : locale_code, pathPrefix: pathPrefix };

		var html = goatee.fill(templates.itemsTemplate, rtnObj);
		var mhtml = goatee.fill(templates.mitemsTemplate, rtnObj);
		items.html(html);
		mitems.html(mhtml);

		/* @T235892: UI adjustments to give the partner search results better context */
		if(searchLabel) {
			sb.find('.search-all').removeClass('show');
			items.find('.search-all').addClass('show').click(function() { $(this).prop('href', '/search/?q=' + form.find(".searchInput").val()); });
			items.find('.search-results-view-all .search-label').html('<span data-i18n="search.in">in</span> ');
			items.find('.search-results-view-all .search-label').append(searchLabel);

			if(!resultCount) {
				sb.find('.search-all').addClass('show');
				items.find('.search-all').removeClass('show');
			}
		}

		$(document).trigger('translate');

		sb.find(".search_result_hero img").error(function() {
			var img = $(this);
			var src = img.prop("src");

			if(src.match(/media\.tellus\.no\/images\/.+t%3D4/)) {
				src = src.replace("t%3D4", "t%3D1");
				img.prop("src", src);
			} else {
				img.remove();
			}
		});

		// for nav results, retrieve a new image that uses focalPoint cropping
		// this needs to be done after the html is updated on the page, to overwrite the existing image
		if (rtnObj.response && rtnObj.response.docs && rtnObj.response.docs.length) {
			if (rtnObj.response.docs[0].type === 'nav' && rtnObj.response.docs[0].image_id) {
				getImage(rtnObj.response.docs[0].image_id.toString());
			} else {
				loadImage();
			}
		}
	};

	// don't enable autocomplete if the widget is in the content area
	if (form.parents('#sv-scroll-to-content-top').length === 0) {
		// waits to call the keyup callback function until 250ms
		// after the last keyup was encountered.
		// prevents querying every single letter as the user types
		// @T237513 - autosuggest not kicking in when typing special characters
		var mobile;
		var prevAutosuggestQuery = '';
		inp.add(minp).keyup(lodash.debounce(function(e) {
			mobile = $(this).is(minp);
			if(mobile) inp.val(minp.val());
			var newQuery = inp.val().trim().replace(/\s+/g, ' ');
			if(!newQuery) {
				items.add(mitems).html("");
				prevAutosuggestQuery = '';
			} else {
				if(e.keyCode === 13) {
					sb.find(".searchBtn").click();
				} else if (newQuery !== prevAutosuggestQuery) {
					// remember what we last queried to avoid submitting if the q hasn't actually changed
					prevAutosuggestQuery = newQuery;
					form.submit();
				}
			}
		}, 250));
	} else {
		// handle pressing the enter key to search in the main content search box
		inp.keyup(lodash.debounce(function(e) {
			if (e.keyCode === 13) {
				sb.find(".searchBtn").click();
			}
		}, 250));
	}

	form.submit(function(e) {
		var action = $(this).attr("action");
		if(!action) {
			e.preventDefault();
			runSearch({ q : inp.val() });
		}
	});

	sb.on("click", ".searchBtn", function() {
		form.attr("action", form.attr("data-action"));
		form.submit();
	});

	sb.on("click", "[data-search-view-all]", function() {
		form.attr("action", form.attr("data-action"));
		form.submit();
	});

	sb.on("click", "[data-sv-searchLink]", function() {
		var query = $(this).attr("data-href");

		window.location.href = window.location.origin + "/search" + query;
	});
});
